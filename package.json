{
	"name": "tour-de-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"pocketbase": "sudo ./pocketbase-server serve --http 0.0.0.0:80",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typegen": "npx pocketbase-typegen --db ./pb_data/data.db --out ./src/lib/pocketbase/types.ts"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@svelte-plugins/tooltips": "^0.1.6",
		"@sveltejs/adapter-node": "^1.0.0-next.104",
		"@sveltejs/adapter-static": "^1.0.5",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.20",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.53.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-loading-spinners": "^0.3.4",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@devexperts/remote-data-ts": "^2.1.1",
		"@lukeed/uuid": "^2.0.0",
		"@zerodevx/svelte-toast": "^0.8.2",
		"d3-scale": "^4.0.2",
		"flatpickr": "^4.6.13",
		"fp-ts": "^2.16.1",
		"iconify-icon": "^1.0.2",
		"pocketbase": "^0.9.1",
		"sass": "^1.57.1",
		"svelte-multiselect": "^8.2.4",
		"ts-node": "^10.9.1"
